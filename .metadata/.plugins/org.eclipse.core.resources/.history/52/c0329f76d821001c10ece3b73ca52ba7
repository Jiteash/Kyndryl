package lnv;
import java.util.*;
public class pycs {


	public static void main(String args[]) {

		String s = "abc123@#$";
		String al= "abc";
		String no="123";
		String spec= "@#$";
		int count;

		for (int i= 0; i<s.length()-1;i++) {

			char out = s.charAt(i);

			for(int j=0; j<al.length()-1;j++) {
				char alout=al.charAt(j);
				char noout=no.charAt(j);
				char specout=spec.charAt(j);
				

					//System.out.println(out);

					if(out==alout) {

						System.out.println(out);
					}
					else if(out==noout){
						System.out.println(out);
					}
					else if(out == specout) {
						System.out.println(out);
					}
					else {
						System.out.println(out);
					}

				}
			}
		}
	

	private static void elseif() {
		// TODO Auto-generated method stub

	}
}